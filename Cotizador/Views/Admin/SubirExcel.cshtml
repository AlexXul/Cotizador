@{
    ViewData["Title"] = "Subir Excel";
}
<div>
    @ViewBag.Mensaje
</div>
<form asp-action="SubirExcel" enctype="multipart/form-data">
    <div class="form-group">
        <input type="hidden" name="name" value="dummy" />
    </div>
    <div class="form-group">
        <label class="control-label">Seleccionar archivo</label>
        <input name="excel" type="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel,text/comma-separated-values, text/csv, application/csv" />
    </div>
    <div class="form-group">
        <input class="btn btn-success" type="submit" value="Subir" />
    </div>
</form>