@{
    ViewData["Title"] = "Opciones";
}

<p class="text-danger">@ViewBag.Mensaje</p>

<a class="btn btn-info" href="/Admin/SubirExcel">Subir Excel</a>
<a class="btn btn-danger" href="/Admin/VerDatosExcel">Ver datos del excel</a>
<a class="btn btn-dark" href="/Admin/ExportarDatos">Exportar datos del excel a la BD</a>


@section scripts {
    <script>
        $(document).ready(function () {
            $.ajax({
                type: "GET",
                url: "@Url.Action("Ejemplo","Admin")",
                content: "application/json; charset=utf-8",
                dataType: "json",
                data: {},
                success: function (d) {
                    console.log(d)
                },
                error: function (xhr, textStatus, errorThrown) {
                    alert('Error!!');
                }
            });
        });
    </script>
}